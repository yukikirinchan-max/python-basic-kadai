      "source": [
        "def calc_tax_included(price, tax_rate):\n",
        "    result = price * (1 + tax_rate / 100)\n",
        "    return result\n",
        "\n",
        "price = 110\n",
        "tax_rate = 10\n",
        "\n",
        "total_price = calc_tax_included(price, tax_rate)\n",
        "\n",
        "# 結果の出力\n",
        "print(f\"消費税込みの金額は {int(total_price)} 円です。\")\n"
      ]
    }
  ]
